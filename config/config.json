{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "apsrdms-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/apsrdms/ApsrdmsWebPart.js",
          "manifest": "./src/webparts/apsrdms/ApsrdmsWebPart.manifest.json"
        }
      ]
    },
    "dmswebaspr-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dmswebaspr/DmswebasprWebPart.js",
