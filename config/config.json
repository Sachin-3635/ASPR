      "components": [
        {
          "entrypoint": "./lib/webparts/apsrdmsSolution/ApsrdmsSolutionWebPart.js",
          "manifest": "./src/webparts/apsrdmsSolution/ApsrdmsSolutionWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ApsrdmsWebPartStrings": "lib/webparts/apsrdms/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
